{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:all": "APP_NAME=ALL rollup -c",
    "build:main": "APP_NAME=MAIN rollup -c",
    "build:two_main": "APP_NAME=TWO_MAIN rollup -c",
    "dev:all": "APP_NAME=ALL rollup -c -w",
    "dev:main": "APP_NAME=MAIN rollup -c -w",
    "dev:two_main": "APP_NAME=TWO_MAIN rollup -c -w",
    "start": "sirv --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{svelte,ts}",
    "lint": "eslint './src/**/*.{ts,svelte}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,svelte}'",
    "prepare": "husky install",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "npm run check",
      "npm run format",
      "npm run lint:fix",
      "npm run test"
    ]
  },
  "type": "module",
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@ota-meshi/eslint-plugin-svelte": "^0.13.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/google.maps": "^3.46.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "eslint-plugin-testing-library": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-eslint-parser": "^0.6.0",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.0.7",
    "tslib": "^2.0.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.8",
    "@types/jest": "^27.0.3",
    "sirv-cli": "^1.0.0"
  }
}
